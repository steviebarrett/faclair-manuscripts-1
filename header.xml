<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml"
	schematypens="http://purl.oclc.org/dsdl/schematron"?>

<?xml-stylesheet type="text/xsl" href="stylesheet.xsl"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
<teiHeader>
	<fileDesc>
		<titleStmt>
			<title>
				<!-- title of the transcription -->
			</title>
			<respStmt>
				<name> <!-- name --> </name>
				<resp>Encoder</resp>
			</respStmt>
		</titleStmt>
		<editionStmt>
			<edition>
				<respStmt>
					<name> <!-- name --> </name>
					<resp>Editor</resp>
				</respStmt>
			</edition>
		</editionStmt>
		<extent>
			<!-- extent of transcription, in words -->
		</extent>
		<publicationStmt>
			<authority>
				<!-- Information about distribution of the resource from corpusHeader (@sameAs) -->
			</authority>
			<availability>
				<!-- Information on availability from corpusHeader (@sameAs) -->
			</availability>
			<idno>
				<!-- idno, for this transcription -->
			</idno>
		</publicationStmt>
		<notesStmt>
			<note>
				<!-- summary of the transcription, covering contents and particular usefulness/interest within the corpus -->
			</note>
		</notesStmt>
		<sourceDesc>
			<msDesc>
				<!-- manuscript information  -->
			</msDesc>
			<listBibl>
				<!-- bibliography of literature relevant to this text, from corpusHeader (<bibl @sameAs> -->
			</listBibl>
		</sourceDesc>
	</fileDesc>
	<encodingDesc>
		<projectDesc>
			<!-- from corpusHeader (@sameAs) -->
		</projectDesc>
		<samplingDecl>
			<!-- from corpusHeader (@sameAs) -->
		</samplingDecl>
		<editorialDecl>
			<!-- from corpusHeader (@sameAs) -->
		</editorialDecl>
		<tagsDecl>
			<!-- from corpusHeader (@sameAs) -->
		</tagsDecl>
		<refsDecl>
			<!-- how references work for this text -->
		</refsDecl>		
	</encodingDesc>
	<profileDesc>
		<creation>
			<!-- authorship of text -->
		</creation>
		<handNotes>
			<!-- relevant handNotes from corpusHeader (@sameAs) -->
		</handNotes>
		<langUsage>
			<!-- langUsage -->
		</langUsage>
		<textClass>
			<!-- keywords -->
		</textClass>
	</profileDesc>
	<revisionDesc>
		<!-- revisions -->
	</revisionDesc>
</teiHeader>
<text>
	<!-- text -->
</text>
</TEI>